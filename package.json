    "postinstall": "yarn clean && yarn build"
  },
  "dependencies": {
    "parcel-bundler": "^1.10.3",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "styled-components": "^4.1.3",
    "suncalc": "^1.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0"
  }
